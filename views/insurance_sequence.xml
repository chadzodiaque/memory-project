<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_insurance_security_product" model="ir.sequence">
            <field name="name">Police insurance</field>
            <field name="code">policy.details</field>
            <field name="prefix">INS/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_insurance_security_demand" model="ir.sequence">
            <field name="name">Demand insurance</field>
            <field name="code">demand.details</field>
            <field name="prefix">DEM/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_insurance_security_claims" model="ir.sequence">
            <field name="name">Claims insurance</field>
            <field name="code">claims.details</field>
            <field name="prefix">CLM/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_insurance_security_payments" model="ir.sequence">
            <field name="name">Payments insurance</field>
            <field name="code">payments.details</field>
            <field name="prefix">PAY/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>